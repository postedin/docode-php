{
  "name": "postedin/docode-php",
  "description": "Library which consumes the https://docode.cl API",
  "keywords": ["docode", "api", "library"],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/postedin/docode-php/issues",
    "source": "https://github.com/postedin/docode-php"
  },
  "authors": [
    {
      "name": "Postedin SpA",
      "email": "contact@postedin.com"
    }
  ],
  "require": {
    "php": ">=7.0.18",
    "guzzlehttp/guzzle": "~6.3",
    "illuminate/support": ">=4.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.9",
    "laktak/hjson": "^2.0",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~6.4"
  },
  "autoload": {
    "psr-4": {
      "Postedin\\Docode\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Postedin\\Docode\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true
}
